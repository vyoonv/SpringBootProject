<mapper namespace="edu.kh.project.board.model.mapper.BoardMapper">
	
	<!-- 게시판 종류 조회 -->
	<select id="selectBoardTypeList">
		SELECT BOARD_CODE "boardCode", BOARD_NAME "boardName"
		FROM BOARD_TYPE 
		ORDER BY BOARD_CODE
	</select>


</mapper>
